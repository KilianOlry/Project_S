{% extends 'base-admin.html.twig' %}

{% block title %}Contrat index{% endblock %}

{% block body %}

<div class="page-header">
			<h1>Tableau de Bord</h1>
		</div>
				<div class="page-content">
					<div class="records table-responsive">

						<div class="record-header">
							<div class="add">
								<span>Entries</span>
								<select name="" id="">
									<option value="">ID</option>
								</select>
								<a href="{{ path('app_souscription_new') }}">
									<button>
										<span>
											<svg xmlns="http://www.w3.org/2000/svg" viewbox="0 0 640 512" width="25px" height=" 25px" class="svg_admin_orange"><!--! Font Awesome Pro 6.3.0 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2023 Fonticons, Inc. --><path d="M96 128a128 128 0 1 1 256 0A128 128 0 1 1 96 128zM0 482.3C0 383.8 79.8 304 178.3 304h91.4C368.2 304 448 383.8 448 482.3c0 16.4-13.3 29.7-29.7 29.7H29.7C13.3 512 0 498.7 0 482.3zM504 312V248H440c-13.3 0-24-10.7-24-24s10.7-24 24-24h64V136c0-13.3 10.7-24 24-24s24 10.7 24 24v64h64c13.3 0 24 10.7 24 24s-10.7 24-24 24H552v64c0 13.3-10.7 24-24 24s-24-10.7-24-24z"/></svg>
										</span>Ajouter Contrat</button>
								</a>
							</div>
							<div class="browse">
								<input type="search" placeholder="Rechercher" class="record-search">
								<select name="" id="">
									<option value="">Statut</option>
								</select>
							</div>
						</div>

						<div>
							<table width="100%">
								<thead>
									<tr>
										<th>ID</th>
                                            <th>NOM/PRÉNOM</th>
                                            <th>CIVILITE</th>
                                            <th>TÉLÉPHONE</th>
                                            <th>EMAIL</th>
                                            <th>PAYS DE NAISSANCE</th>
                                            <th>ADRESSE</th>
                                            <th>CODE POSTALE</th>
                                            <th>VILLE</th>
                                            <th>SITUATION</th>
                                            <th>NUMÉRO DE RÉSERVISTE</th>
                                            <th>ACTIONS</th>
									</tr>
								</thead>
								<tbody>
									{% for contrat in contrats %}
										<tr>
											<td>{{ contrat.id }}</td>
											<td>
												<div
													class="client">
													{# <div class="client-img bg-img" style="background-image: url(img/3.jpeg)"></div> #}
													<div class="client-info">
														<h4>{{  contrat.prenom}}</h4>
														<small>{{ contrat.nom }}</small>
													</div>
												</div>
											</td>
											<td>
											    {{contrat.civilite}}
											</td>
											<td>
                                            	{{contrat.telephone}}
											</td>
											<td>
                                            {{contrat.email}}
											</td>
											<td>
                                            {{ contrat.paysNaissance }}
											</td>
											<td>
												
                                                {{contrat.adresse}}
											</td>
											<td>
												{{contrat.codePostale}}
											</td>
											<td>
												{{contrat.ville}}
											</td>
											<td>
												{{contrat.situation}}
											</td>
											<td>
												{{ contrat.nbReserviste }}
											</td>
											<td>
												<div class="actions">
												<a href="{{ path('app_souscription_pdf', {'id': contrat.id}) }}">
														<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" width="20px" height=" 20px" class="svg_admin_blue"><!--! Font Awesome Pro 6.4.0 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2023 Fonticons, Inc. --><path d="M0 64C0 28.7 28.7 0 64 0H224V128c0 17.7 14.3 32 32 32H384V304H176c-35.3 0-64 28.7-64 64V512H64c-35.3 0-64-28.7-64-64V64zm384 64H256V0L384 128zM176 352h32c30.9 0 56 25.1 56 56s-25.1 56-56 56H192v32c0 8.8-7.2 16-16 16s-16-7.2-16-16V448 368c0-8.8 7.2-16 16-16zm32 80c13.3 0 24-10.7 24-24s-10.7-24-24-24H192v48h16zm96-80h32c26.5 0 48 21.5 48 48v64c0 26.5-21.5 48-48 48H304c-8.8 0-16-7.2-16-16V368c0-8.8 7.2-16 16-16zm32 128c8.8 0 16-7.2 16-16V400c0-8.8-7.2-16-16-16H320v96h16zm80-112c0-8.8 7.2-16 16-16h48c8.8 0 16 7.2 16 16s-7.2 16-16 16H448v32h32c8.8 0 16 7.2 16 16s-7.2 16-16 16H448v48c0 8.8-7.2 16-16 16s-16-7.2-16-16V432 368z"/></svg>
													</a>
												<a href="{{ path('app_souscription_show', {'id': contrat.id}) }}">
														<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 576 512"width="20px" height=" 20px" class="svg_admin_orange"><!--! Font Awesome Pro 6.3.0 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2023 Fonticons, Inc. --><path d="M288 32c-80.8 0-145.5 36.8-192.6 80.6C48.6 156 17.3 208 2.5 243.7c-3.3 7.9-3.3 16.7 0 24.6C17.3 304 48.6 356 95.4 399.4C142.5 443.2 207.2 480 288 480s145.5-36.8 192.6-80.6c46.8-43.5 78.1-95.4 93-131.1c3.3-7.9 3.3-16.7 0-24.6c-14.9-35.7-46.2-87.7-93-131.1C433.5 68.8 368.8 32 288 32zM144 256a144 144 0 1 1 288 0 144 144 0 1 1 -288 0zm144-64c0 35.3-28.7 64-64 64c-7.1 0-13.9-1.2-20.3-3.3c-5.5-1.8-11.9 1.6-11.7 7.4c.3 6.9 1.3 13.8 3.2 20.7c13.7 51.2 66.4 81.6 117.6 67.9s81.6-66.4 67.9-117.6c-11.1-41.5-47.8-69.4-88.6-71.1c-5.8-.2-9.2 6.1-7.4 11.7c2.1 6.4 3.3 13.2 3.3 20.3z"/></svg>
													</a>
													<a href="{{ path('app_souscription_edit', {'id': contrat.id}) }}">
														<svg xmlns="http://www.w3.org/2000/svg" viewbox="0 0 512 512" width="20px" height=" 20px" class="svg_admin_blue"><!--! Font Awesome Pro 6.3.0 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2023 Fonticons, Inc. --><path d="M441 58.9L453.1 71c9.4 9.4 9.4 24.6 0 33.9L424 134.1 377.9 88 407 58.9c9.4-9.4 24.6-9.4 33.9 0zM209.8 256.2L344 121.9 390.1 168 255.8 302.2c-2.9 2.9-6.5 5-10.4 6.1l-58.5 16.7 16.7-58.5c1.1-3.9 3.2-7.5 6.1-10.4zM373.1 25L175.8 222.2c-8.7 8.7-15 19.4-18.3 31.1l-28.6 100c-2.4 8.4-.1 17.4 6.1 23.6s15.2 8.5 23.6 6.1l100-28.6c11.8-3.4 22.5-9.7 31.1-18.3L487 138.9c28.1-28.1 28.1-73.7 0-101.8L474.9 25C446.8-3.1 401.2-3.1 373.1 25zM88 64C39.4 64 0 103.4 0 152V424c0 48.6 39.4 88 88 88H360c48.6 0 88-39.4 88-88V312c0-13.3-10.7-24-24-24s-24 10.7-24 24V424c0 22.1-17.9 40-40 40H88c-22.1 0-40-17.9-40-40V152c0-22.1 17.9-40 40-40H200c13.3 0 24-10.7 24-24s-10.7-24-24-24H88z"/></svg>
													</a>
													<a href="{{ path('app_admin_user_delete', {'id': contrat.id}) }}">
														<svg xmlns="http://www.w3.org/2000/svg" viewbox="0 0 448 512" width="20px" height=" 20px" class="svg_admin_orange"><!--! Font Awesome Pro 6.3.0 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2023 Fonticons, Inc. --><path d="M163.8 0H284.2c12.1 0 23.2 6.8 28.6 17.7L320 32h96c17.7 0 32 14.3 32 32s-14.3 32-32 32H32C14.3 96 0 81.7 0 64S14.3 32 32 32h96l7.2-14.3C140.6 6.8 151.7 0 163.8 0zM32 128H416V448c0 35.3-28.7 64-64 64H96c-35.3 0-64-28.7-64-64V128zm192 64c-6.4 0-12.5 2.5-17 7l-80 80c-9.4 9.4-9.4 24.6 0 33.9s24.6 9.4 33.9 0l39-39V408c0 13.3 10.7 24 24 24s24-10.7 24-24V273.9l39 39c9.4 9.4 24.6 9.4 33.9 0s9.4-24.6 0-33.9l-80-80c-4.5-4.5-10.6-7-17-7z"/></svg>
													</a>
												</div>
											</td>
										</tr>
									{% else %}
										<tr>
											<td colspan="10">no records found</td>
										</tr>
									{% endfor %}
								</tbody>
							</table>
						</div>
					</div>
        {# {% for contrat in contrats %}
            <tr>
                <td>{{ contrat.nomNaissance }}</td>
                <td>{{ contrat.paysNaissance }}</td>
                <td>{{ contrat.nbReserviste }}</td>
                <td>{{ contrat.dateDelivrance ? contrat.dateDelivrance|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ contrat.reservisteFiles }}</td>
                <td>{{ contrat.dateGarantie ? contrat.dateGarantie|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ contrat.pdfNoFirm }}</td>
                <td> #}

{% endblock %}


        