{% extends 'base.html.twig' %}

{% block title %}Contact
{% endblock %}

{% block stylesheets %}
	<link rel="stylesheet" href="{{ asset('assets/css/contact.css') }}">
{% endblock %}

{% block body %}
	<div class="slide"></div>


	<div class="contactUs">
		<div class="title">
			<h1>Nous contacter</h1>
		</div>
		<div class="box">
			<div class="contact form">
				<h3>Envoyer votre message</h3>
				{{ form_start(form) }}
				<div class="formBox">
					<div class="row50">
						<div class="inputBox">
							{{ form_row(form.email, {'attr': {'class':'inputBox'}}) }}
						</div>
					</div>
					<div class="row50">
						<div class="inputBox">
							{{ form_row(form.objet, {'attr': {'class':'inputBox'}}) }}
						</div>
					</div>
					<div class="row100">
						<div class="inputBox">
							{{ form_row(form.message, {'attr': {'class':'inputBox'}}) }}
						</div>
					</div>
					<div class="row100">
						<div class="inputBox">
							{{ form_row(form.envoi, {'attr': {'class':'submit'}}) }}
							{{ form_end(form) }}
						</div>
					</div>
				</div>
			</div>
			<div class="contact info">
				<h3>Contact info</h3>
				<div class="infoBox">
					<div>
						<span>
